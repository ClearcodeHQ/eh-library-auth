{
    "name": "clearcode/eh-library-auth",
    "description": "Model and use cases for rest api course",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Clearcode",
            "homepage": "http://clearcode.cc/"
        }
    ],
    "autoload": {
        "psr-4" : {
            "Clearcode\\EHLibraryAuth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "tests\\Clearcode\\EHLibraryAuth\\" : "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.5",
        "everzet/persisted-objects": "~1.0",
        "lcobucci/jwt": "~3",
        "ramsey/uuid": "~3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "behat/behat": "~3",
        "squizlabs/php_codesniffer": "~2"
    },
    "config": {
        "bin-dir": "bin"
    }
}
